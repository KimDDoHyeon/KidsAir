<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/service_dashboard"
    android:orientation="vertical">

    <TextView
        android:id="@+id/textView15"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:text="약관 및 정책"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="25dp"
        android:textColor="#A5A5A5" />

    <FrameLayout
        android:id="@+id/policy_1"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:onClick="policy_1_click">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginStart="25dp"
                android:layout_marginLeft="25dp"
                android:layout_weight="1"
                android:text="이용약관"
                android:textColor="#020202"
                android:textSize="18sp" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="wrap_content"
                android:layout_height="24dp"
                android:layout_gravity="center"
                android:layout_marginEnd="25dp"
                android:layout_marginRight="25dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/arrow" />
        </LinearLayout>
    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="0.2dp">

        <View
            android:id="@+id/view4"
            android:layout_width="wrap_content"
            android:layout_height="0.2dp"
            android:background="#707070" />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/policy_2"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:onClick="policy_2_click">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginStart="25dp"
                android:layout_marginLeft="25dp"
                android:layout_weight="1"
                android:text="위치기반 서비스 이용약관"
                android:textColor="#020202"
                android:textSize="18sp" />

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="wrap_content"
                android:layout_height="24dp"
                android:layout_gravity="center"
                android:layout_marginEnd="25dp"
                android:layout_marginRight="25dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/arrow" />
        </LinearLayout>
    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="0.2dp">

        <View
            android:id="@+id/view3"
            android:layout_width="wrap_content"
            android:layout_height="0.2dp"
            android:background="#707070" />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/policy_3"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:onClick="policy_3_click">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginStart="25dp"
                android:layout_marginLeft="25dp"
                android:layout_weight="1"
                android:text="운영정책"
                android:textColor="#020202"
                android:textSize="18sp" />

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="wrap_content"
                android:layout_height="24dp"
                android:layout_gravity="center"
                android:layout_marginEnd="25dp"
                android:layout_marginRight="25dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/arrow" />
        </LinearLayout>
    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="0.2dp">

        <View
            android:id="@+id/view5"
            android:layout_width="wrap_content"
            android:layout_height="0.2dp"
            android:background="#707070" />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/policy_4"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:onClick="policy_4_click">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginStart="25dp"
                android:layout_marginLeft="25dp"
                android:layout_weight="1"
                android:text="개인정보처리방침"
                android:textColor="#020202"
                android:textSize="18sp" />

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="wrap_content"
                android:layout_height="24dp"
                android:layout_gravity="center"
                android:layout_marginEnd="25dp"
                android:layout_marginRight="25dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/arrow" />
        </LinearLayout>
    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="0.2dp"
        android:background="#707070">

        <View
            android:id="@+id/view2"
            android:layout_width="wrap_content"
            android:layout_height="0.2dp"
            android:background="#707070" />
    </FrameLayout>

    <FrameLayout
        android:id="@+id/policy_5"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:onClick="policy_5_click">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginStart="25dp"
                android:layout_marginLeft="25dp"
                android:layout_weight="1"
                android:text="오픈소스 라이센스"
                android:textColor="#020202"
                android:textSize="18sp" />

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="wrap_content"
                android:layout_height="24dp"
                android:layout_gravity="center"
                android:layout_marginEnd="25dp"
                android:layout_marginRight="25dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/arrow" />
        </LinearLayout>
    </FrameLayout>
</LinearLayout>